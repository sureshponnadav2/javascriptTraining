// object constructor
function Employee(firstName, lastName, empId, age) {
    this.firstName = firstName;
    this.lastName = lastName;
    this.empId = empId;
    this.age = age;
};

// prototype

Employee.prototype.section = 'IT';


const emp1 = new Employee('Swapnil', 'Pune', 007, 30);

emp1.location = 'India';
console.log(emp1);

const emp2 = new Employee('Raj', 'Pune', 007, 30);

console.log(emp2);




function returnPromise() {
  var newPromise = new Promise((resolve, reject) => {
    setTimeout(() => {
      console.log("Promise executed");
      resolve("promise resolved");
    }, 3000);
  });
  return newPromise;
}


// with the .then method
function executePromise() {
  const newData = 'swapnil';
  returnPromise().then((res) => {
    console.log(res);
  });
  console.log(newData);
}

// this is with the async await function
